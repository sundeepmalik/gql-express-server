    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@playlyfe/gql": "^2.6.2",
    "ajv": "^5.5.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "cross-env": "^5.2.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2"
  }
